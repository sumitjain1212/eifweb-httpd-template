<Macro MACRO_SNIPPET_PROXYPASS ${MACRO_VH_PROXY1_HOSTPORT}>
  ProxyPass / http://${MACRO_VH_PROXY1_HOSTPORT}/
  ProxyPassReverse / http://${MACRO_VH_PROXY1_HOSTPORT}/
  
  <Location />
    Include ${EIF_APACHE_CONFDIR}/auth.conf
    Require valid-user
    <IfDefine AUTHOIDC>
      OIDCPassRefreshToken Off
	  RequestHeader unset Authorization
	  RequestHeader unset Oidc_access_token
	  RequestHeader unset Oidc_access_token_expires
	</IfDefine>
  </Location>
</Macro>
