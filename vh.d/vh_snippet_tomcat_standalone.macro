<Macro MACRO_SNIPPET_TOMCAT_CLUSTER ${MACRO_VH_PROXY1_HOSTPORT} ${MACRO_VH_PROXY1_NAME}>
  <Proxy balancer://macrobalancer>
    BalancerMember ajp://${MACRO_VH_PROXY1_HOSTPORT} route=${MACRO_VH_PROXY1_NAME}  loadfactor=1
    ProxySet lbmethod=bytraffic
  </Proxy>

  ProxyPass / balancer://macrobalancer/ stickysession=JSESSIONID nofailover=Off

  <Location />
    Include ${EIF_APACHE_CONFDIR}/auth.conf
    Require valid-user
  </Location>
</Macro>
